<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean name="filmService"
		class="by.bsuir.cinema.service.impl.FilmServiceImpl">
		<property name="filmDao" ref="filmDao" />
	</bean>
	<bean name="filmSessionService"
		class="by.bsuir.cinema.service.impl.FilmSessionServiceImpl">
		<property name="filmSessionDao" ref="filmSessionDao" />
	</bean>
	<bean name="genreService"
		class="by.bsuir.cinema.service.impl.GenreServiceImpl">
		<property name="genreDao" ref="genreDao" />
		<property name="filmDao" ref="filmDao" />
	</bean>
	<bean name="roleService"
		class="by.bsuir.cinema.service.impl.RoleServiceImpl">
		<property name="roleDao" ref="roleDao" />
		<property name="userDao" ref="userDao" />
	</bean>
	<bean name="seatService"
		class="by.bsuir.cinema.service.impl.SeatServiceImpl">
		<property name="seatDao" ref="seatDao" />
		<property name="ticketsOrderDao" ref="ticketsOrderDao" />
	</bean>
	<bean name="ticketService"
		class="by.bsuir.cinema.service.impl.TicketServiceImpl">
		<property name="ticketDao" ref="ticketDao" />
	</bean>
	<bean name="ticketOrderService"
		class="by.bsuir.cinema.service.impl.TicketsOrderServiceImpl">
		<property name="ticketsOrderDao" ref="ticketsOrderDao" />
	</bean>
	<bean name="userService"
		class="by.bsuir.cinema.service.impl.UserServiceImpl">
		<property name="userDao" ref="userDao" />
	</bean>

</beans>